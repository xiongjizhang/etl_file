    <sql>select d.ConnectionName,&#xd;&#xa;       concat&#x28;&#x27;SELECT DBID,&#x27;,&#xd;&#xa;       &#x27;SCHEMA_NAME,&#x27;,&#xd;&#xa;       &#x27;DEFAULT_CHARACTER_SET_NAME,&#x27;,&#xd;&#xa;	   &#x27;DEFAULT_COLLATION_NAME &#x27;,&#xd;&#xa;       &#x27;FROM t_meta_schema where DBID&#x3d; &#x27;,d.DBID,&#xd;&#xa;       &#x27; order by 1,2&#x27;&#x29; as t_sql,&#xd;&#xa;       IF&#x28;d.DBTYPE&#x3d;&#x27;mysql&#x27;,&#xd;&#xa;            concat&#x28;&#x27;select &#x27;,d.DBID,&#x27; DBID, &#x27;,   -- mysql&#xd;&#xa;       		&#x27;	t.SCHEMA_NAME SCHEMA_NAME, &#x27;,&#xd;&#xa;       		&#x27;	t.DEFAULT_CHARACTER_SET_NAME, &#x27;,&#xd;&#xa;	   		&#x27;	t.DEFAULT_COLLATION_NAME  &#x27;,&#xd;&#xa;  			&#x27;from information_schema.SCHEMATA t  &#x27;,&#xd;&#xa; 			&#x27;where upper&#x28;t.SCHEMA_NAME&#x29; not in&#x28;upper&#x28;&#x5c;&#x27;information_schema&#x5c;&#x27;&#x29;, &#x27;,&#xd;&#xa;            &#x27;                       upper&#x28;&#x5c;&#x27;mysql&#x5c;&#x27;&#x29;, &#x27;,&#xd;&#xa;			&#x27;					   upper&#x28;&#x5c;&#x27;performance_schema&#x5c;&#x27;&#x29;, &#x27;,&#xd;&#xa;			&#x27;					   upper&#x28;&#x5c;&#x27;test&#x5c;&#x27;&#x29;, &#x27;,&#xd;&#xa;			&#x27;					   upper&#x28;&#x5c;&#x27;sys&#x5c;&#x27;&#x29;, &#x27;,&#xd;&#xa;			&#x27;					   upper&#x28;&#x5c;&#x27;tmp&#x5c;&#x27;&#x29;&#x29;&#x27;&#x29;,&#xd;&#xa;       		concat&#x28;&#x27;SELECT &#x27;,d.DBID,&#x27; DBID,&#x27;,  -- mssql&#xd;&#xa;       		&#x27;name SCHEMA_NAME,&#x27;,&#xd;&#xa;       		&#x27;&#x5c;&#x27;&#x5c;&#x27; DEFAULT_CHARACTER_SET_NAME,&#x27;,&#xd;&#xa;	   		&#x27;&#x5c;&#x27;&#x5c;&#x27; DEFAULT_COLLATION_NAME &#x27;,&#xd;&#xa;       		&#x27;FROM Master..SysDatabases t &#x27;,&#xd;&#xa;       		&#x27;where upper&#x28;t.name&#x29; not in&#x28;upper&#x28;&#x5c;&#x27;master&#x5c;&#x27;&#x29;,&#x27;,&#xd;&#xa;       		&#x27;                    upper&#x28;&#x5c;&#x27;model&#x5c;&#x27;&#x29;,&#x27;,&#xd;&#xa;			&#x27;				   upper&#x28;&#x5c;&#x27;msdb&#x5c;&#x27;&#x29;,&#x27;,&#xd;&#xa;			&#x27;				   upper&#x28;&#x5c;&#x27;tempdb&#x5c;&#x27;&#x29;&#x29; &#x27;,&#xd;&#xa;       		&#x27;order by 1,2&#x27;&#x29;&#x29; as s_sql&#xd;&#xa;from etl.tconf_DataBase d&#xd;&#xa;inner join etl.tconf_schedule_type st on d.InterfaceID &#x3d; st.InterfaceID&#xd;&#xa; where d.DBTYPE&#x3d; &#x27;&#x24;&#x7b;USER_DBTYPE&#x7d;&#x27; &#xd;&#xa;and st.InterfaceType &#x3d; &#x27;&#x24;&#x7b;USER_INTERFACETYPE&#x7d;&#x27;&#xd;&#xa;and d.DBID &#x3d; &#x27;&#x24;&#x7b;USER_DBID&#x7d;&#x27;</sql>
